<% title "New" %>

<% form_for(@sub_commission) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :term %><br />
    <%= f.text_field :term %>
  </p>
  <p>
    <%= f.label :policy_year %><br />
    <%= f.text_field :policy_year %>
  </p>
  <p>
    <%= f.label :percentage %><br />
    <%= f.text_field :percentage %>
  </p>
  <p>
    <%= f.submit 'Create' %> | <%= link_to 'Back', sub_commissions_path %>
  </p>
<% end %>

