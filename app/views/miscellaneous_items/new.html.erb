
<% title "New miscellaneous" %>
<br />
<%= link_to 'Back', miscellaneous_items_path %>
<br /><br />

<% form_for(@miscellaneous_item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :transaction_date %><br />
    <%= f.date_select :transaction_date %>
  </p>
  <p>
    <%= f.label :agent_id %><br />
	<%= f.text_field_with_auto_complete :agent_name, {}, { :url => show_agent_miscellaneous_items_path, :method => :get, :skip_style => true, :with => "'search='+element.value" } %>
  </p>
  
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :size => "80x8" %>
  </p>

  <p>
    <%= f.label :amount %><br />
    <%= f.text_field :amount %>
  </p>
  <p>
    <%= f.label :builtin %><br />
    <%= f.check_box :builtin %>
  </p>
   <p>
    <%= f.label :overriding_charger %><br />
    <%= f.check_box :overriding_charger %>
  </p>

  
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

