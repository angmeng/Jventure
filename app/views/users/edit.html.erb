<% title "Edit User" %>

<br />

<% form_for @user do |f| %>
  <%= f.error_messages %>

<p>
    <%= f.label :fullname %><br />
    <%= f.text_field :fullname %>
  </p>

  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>
  

	
  <p>
    <%= f.label :email, "Email Address" %><br />
    <%= f.text_field :email %>
  </p>

  <br />
  
  <p><%= f.submit "Update" %></p>
<% end %>


